package Entity;

import TileMap.TileMap;

public abstract class MapObject {
	
	protected TileMap tilemap;
	protected int tilesize;
	protected double xmap;
	protected double ymap;
	
	protected double x;
	protected double y;
	protected double dx;
	protected double dy;
	
	protected int ancho;
	protected int alto;
	
	protected int colancho;
	protected int colalto;
	
	
	
}
